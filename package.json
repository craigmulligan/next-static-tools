{
  "name": "next-static-tools",
  "version": "0.0.0-development",
  "description": "Tools for building grapqhl static sites with next.js",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint './src/**/*.js'",
    "build": "babel src --out-dir dist --copy-files",
    "build:watch": "babel src --watch --out-dir dist --copy-files",
    "prettier": "prettier --no-semi --single-quote --write './src/**/*.js'",
    "prettier-check": "prettier-check --no-semi --single-quote --write './src/**/*.js'",
    "semantic-release": "semantic-release",
    "docs": "jsdoc2md 'src/**/*.js' > DOCUMENTATION.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hobochild/next-static-tools.git"
  },
  "keywords": [
    "next.js",
    "static-site",
    "graphql"
  ],
  "author": "hobochild",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hobochild/next-static-tools/issues"
  },
  "browser": {
    "fs-jetpack": false
  },
  "files": [
    "dist/",
    "withData.js",
    "link.js",
    "client.js"
  ],
  "homepage": "https://github.com/hobochild/next-static-tools#readme",
  "dependencies": {
    "apollo-client-preset": "^1.0.1",
    "apollo-error-overlay": "^1.0.1",
    "apollo-link-error": "^1.0.3",
    "apollo-server-express": "^1.3.2",
    "async-on-exit": "^0.1.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "fs-jetpack": "^1.2.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.18.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "^4.2.3",
    "prop-types": "^15.5.8",
    "prop-types-exact": "^1.1.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.0",
    "react-dom": "^16.2.0",
    "sw-precache": "^5.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "prettier": "^1.9.2",
    "prettier-check": "^2.0.0",
    "semantic-release": "^11.0.2"
  },
  "peerDependencies": {
    "next": "^4.2.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
